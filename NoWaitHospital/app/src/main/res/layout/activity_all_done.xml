<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.uchealth.healthhack.nowaithospital.DoneQuestionsActivity"
    android:background="#289473">

    <ImageView
        android:id="@+id/check_iamge"
        android:layout_width="70dp"
        android:layout_height="150dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="80dp"
        android:src="@drawable/success"/>

    <TextView
        android:id="@+id/more_detail_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="All Done.\nReach selected medical care soon!"
        android:textColor="#ffffff"
        android:layout_below="@+id/check_iamge"
        android:layout_centerHorizontal="true"
        android:layout_marginRight="35dp"
        android:layout_marginLeft="35dp"
        android:textAlignment="center"
        android:layout_marginTop="75dp"
        android:textSize="30dp"/>
</RelativeLayout>
